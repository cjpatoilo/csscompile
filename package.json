{
  "name": "csscompile",
  "version": "0.2.0",
  "description": "Compile, optimize and compress anything to CSS.",
  "homepage": "https://github.com/cjpatoilo/csscompile",
  "repository": "cjpatoilo/csscompile",
  "license": "MIT",
  "author": "CJ Patoilo <cjpatoilo@gmail.com>",
  "bin": "cli.js",
  "main": "index.js",
  "files": [
    "cli.js",
    "index.js"
  ],
  "keywords": [
    "automation",
    "autoprefixer",
    "cli",
    "compile",
    "compress",
    "css",
    "cssnano",
    "framework",
    "less",
    "minified",
    "minify",
    "npm",
    "optimize",
    "package",
    "postcss",
    "render",
    "sass",
    "scss",
    "stylus"
  ],
  "dependencies": {
    "gulp": "^4.0.2",
    "gulp-cssmin": "^0.2.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-less": "^4.0.1",
    "gulp-sass": "^4.1.0",
    "gulp-stylus": "^2.7.0"
  },
  "devDependencies": {
    "ava": "^3.11.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier-standard": "^16.4.1"
  },
  "scripts": {
    "lint": "prettier-standard --check",
    "test": "ava"
  },
  "engines": {
    "node": "^12.18.0",
    "npm": "^6.14.5"
  },
  "prettier": {
    "jsxSingleQuote": false,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": [
      "prettier-standard --format",
      "git add"
    ]
  }
}
