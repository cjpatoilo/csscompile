{
	"name": "csscompile",
	"version": "0.1.0",
	"description": "Compile, optimize and compress anything to CSS.",
	"homepage": "https://github.com/cjpatoilo/csscompile",
	"repository": "cjpatoilo/csscompile",
	"license": "MIT",
	"author": "CJ Patoilo <cjpatoilo@gmail.com>",
	"bin": "bin/cli.js",
	"main": "src/index.js",
	"files": [
		"bin/cli.js",
		"src/index.js"
	],
	"keywords": [
		"automation",
		"autoprefixer",
		"cli",
		"compile",
		"compress",
		"css",
		"cssnano",
		"framework",
		"less",
		"minified",
		"npm",
		"optimize",
		"package",
		"postcss",
		"render",
		"sass",
		"scss",
		"stylus"
	],
	"ignore": [
		".appveyor.yml",
		".editorconfig",
		".github",
		".gitignore",
		".travis.yml"
	],
	"dependencies": {
		"gulp": "^3.9.1",
		"gulp-cssnano": "^2.1.2",
		"gulp-less": "^3.3.0",
		"gulp-sass": "^3.1.0",
		"gulp-stylus": "^2.6.0"
	},
	"devDependencies": {
		"ava": "^0.17.0",
		"editorconfig-tools": "^0.1.1",
		"eslint": "^3.12.1",
		"eslint-config-styled": "^0.0.0",
		"husky": "^0.12.0",
		"nyc": "^10.0.0"
	},
	"engines": {
		"node": ">=4"
	},
	"scripts": {
		"precommit": "npm test",
